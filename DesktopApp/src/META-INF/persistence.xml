<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="DesktopAppPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.DesktopApp.Entity.User</class>
    <class>com.DesktopApp.Entity.Role</class>
    <class>com.DesktopApp.Entity.UserGroup</class>
    <class>com.DesktopApp.Entity.Province</class>
    <class>com.DesktopApp.Entity.UserRole</class>
    <class>com.DesktopApp.Entity.SubRedRed</class>
    <class>com.DesktopApp.Entity.SubRed</class>
    <class>com.DesktopApp.Entity.Group</class>
    <class>com.DesktopApp.Entity.Red</class>
    <class>com.DesktopApp.Entity.LeaderGroup</class>
    <class>com.DesktopApp.Entity.District</class>
    <class>com.DesktopApp.Entity.RolePermission</class>
    <class>com.DesktopApp.Entity.Canton</class>
    <class>com.DesktopApp.Entity.Permission</class>
    <class>com.DesktopApp.Entity.Consolidation</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/ccivida?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.password" value="rocketadmin"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
